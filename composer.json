    "version": "0.0.5",
    "require": {
      "php": "~7.3.0|~7.4.0"
    },
    "type": "magento2-module",
    "license": "proprietary" ,
    "autoload": {
      "psr-4": {
        "Nexio\\OnlinePayment\\": ""
      }
    },
    "repositories": [{
